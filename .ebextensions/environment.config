option_settings:
  - namespace: aws:elasticbeanstalk:application:environment
    option_name: IS_EBS_ENV
    value: 1
container_commands:
  04setServerPermissions1:
    command: setfacl -R -m u:elasticbeanstalk:rwX app/cache app/logs
  05setCLIPermissions1:
    command: setfacl -R -m u:ec2-user:rwX app/cache  app/logs
  06setServerPermissions2:
    command: setfacl -dR -m u:elasticbeanstalk:rwX app/cache  app/logs
  07setCLIPermissions2:
    command: setfacl -dR -m u:ec2-user:rwX app/cache  app/logs