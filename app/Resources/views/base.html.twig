<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="{{ asset('bundles/framework/css/bootstrap.css') }}" rel="stylesheet" type="text/css" media="all" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        
        <!-- Uncompressed DEV JQuery -->
        <script src="{{ asset('bundles/framework/js/jquery-2.1.0.js') }}"></script>
        
        
        <!-- Bootstap JS Files -->
        <script src="{{ asset('bundles/framework/js/bootstrap/affix.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/alert.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/button.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/carousel.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/collapse.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/dropdown.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/tab.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/transition.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/scrollspy.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/modal.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/tooltip.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/bootstrap/popover.js') }}"></script>
         
        {% endblock %}
    </body>
</html>
